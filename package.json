{
  "name": "hello-books",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": " set NODE_ENV=test&& sequelize db:migrate:undo:all && sequelize db:migrate && nyc --reporter=html --reporter=text mocha --compilers js:babel-core/register server/tests --timeout 8000 --exit",
    "start": "sequelize db:migrate && sequelize db:seed:all && babel-node app.js",
    "start-dev": "babel-watch app.js",
    "heroku-postbuild": "npm install sequelize-cli babel-cli -g",
    "dev": "webpack-dev-server --colors",
    "watch": "webpack --watch && npm run build",
    "build": "webpack",
    "test:client": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/llabake/Hello-Books.git"
  },
  "keywords": [
    "books",
    "library",
    "borrow"
  ],
  "author": " Lemboye Omolabake",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/llabake/Hello-Books/issues"
  },
  "homepage": "https://github.com/llabake/Hello-Books#readme",
  "jest": {
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "verbose": true,
    "setupFiles": [
      "jest-localstorage-mock",
      "<rootDir>/test-setup.js"
    ],
    "setupTestFrameworkScriptFile": "jest-environment-jsdom"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.7",
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "expect": "^21.2.1",
    "express": "^4.16.2",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^23.0.0-alpha.0",
    "jest-localstorage-mock": "^2.2.0",
    "jsonwebtoken": "^8.1.0",
    "mocha": "^4.0.1",
    "moment": "^2.21.0",
    "morgan": "^1.9.0",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-materialize": "^1.1.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "regenerator-runtime": "^0.11.1",
    "sequelize": "^4.22.5",
    "sinon": "^4.4.6",
    "supertest": "^3.0.0",
    "swagger-jsdoc": "^1.3.0",
    "sweetalert": "^2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "nyc": "^11.2.1",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
